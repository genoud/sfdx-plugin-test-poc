{
  "name": "sfdx-test",
  "description": "Getstarted writing tests for dx plugins",
  "version": "1.0.0",
  "author": "Genoud Magloire DOUANLA DJATIO",
  "bugs": "https://github.com/genoud/sfdx-plugin-test-poc/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@salesforce/command": "^2.1.0",
    "@salesforce/core": "^2.1.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "1.5.0",
    "@types/chai": "^4",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.1.0",
    "chai": "^4",
    "globby": "^10.0.1",
    "mocha": "^6.2.0",
    "mocha-junit-reporter": "^1.23.1",
    "nyc": "^14",
    "rimraf": "^3.0.0",
    "ts-node": "^8",
    "tslint": "^5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/dev/sfdx-test",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "hello": {
        "description": "Commands to say hello."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "dev/sfdx-test",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only --reporter mocha-junit-reporter  \"test/**/*.test.ts\"",
    "coverage": "nyc npm test && nyc report --reporter text-lcov > coverage.lcov",
    "version": "oclif-dev readme && git add README.md"
  }
}
